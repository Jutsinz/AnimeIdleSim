repeat wait() until game:IsLoaded()

local VirtualUser = game:GetService("VirtualUser")

game:GetService("Players").LocalPlayer.Idled:Connect(
    function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
)

local nameOfGame = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)
local Mob = game:GetService("Workspace").Game.PlayerIslands.Jutsinz.spawnedAllies

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = nameOfGame.Name, HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab4 = Window:MakeTab({
	Name = "Auto",
	Icon = "",
	PremiumOnly = false
})

local Tab1 = Window:MakeTab({
	Name = "All",
	Icon = "",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "The",
	Icon = "",
	PremiumOnly = false
})

local Tab3 = Window:MakeTab({
	Name = "Skills",
	Icon = "",
	PremiumOnly = false
})

Tab4:AddToggle({
	Name = "Naruto",
	Default = false,
	Callback = function(Value)
        naruto = Value
        if naruto then
            pcall(function ()
                while naruto and task.wait() do 
                    local string_1 = "LearnSkill";
                    local number_1 = 1;
                    local string_2 = "Shadow clone";
                    local Target = game:GetService("ReplicatedStorage")["_GAME"]["_MODULES"].Utilities.NetworkUtility.Events.Allies;
                    Target:FireServer(string_1, number_1, string_2);
                    task.wait()
                    local string_1 = "LearnSkill";
                    local number_1 = 1;
                    local string_2 = "Six Paths Mode";
                    local Target = game:GetService("ReplicatedStorage")["_GAME"]["_MODULES"].Utilities.NetworkUtility.Events.Allies;
                    Target:FireServer(string_1, number_1, string_2);
                    task.wait()
                    local string_1 = "LearnSkill";
                    local number_1 = 1;
                    local string_2 = "Rasengan";
                    local Target = game:GetService("ReplicatedStorage")["_GAME"]["_MODULES"].Utilities.NetworkUtility.Events.Allies;
                    Target:FireServer(string_1, number_1, string_2);
                    task.wait()
                    local string_1 = "LearnSkill";
                    local number_1 = 1;
                    local string_2 = "Rasenshuriken";
                    local Target = game:GetService("ReplicatedStorage")["_GAME"]["_MODULES"].Utilities.NetworkUtility.Events.Allies;
                    Target:FireServer(string_1, number_1, string_2);
                    task.wait()
                    local string_1 = "LearnSkill";
                    local number_1 = 1;
                    local string_2 = "Sage Mode";
                    local Target = game:GetService("ReplicatedStorage")["_GAME"]["_MODULES"].Utilities.NetworkUtility.Events.Allies;
                    Target:FireServer(string_1, number_1, string_2);
                    task.wait()
                    local string_1 = "LearnSkill";
                    local number_1 = 1;
                    local string_2 = "Baryon Mode";
                    local Target = game:GetService("ReplicatedStorage")["_GAME"]["_MODULES"].Utilities.NetworkUtility.Events.Allies;
                    Target:FireServer(string_1, number_1, string_2);
                    task.wait()
                    break
                end   
            end)
        end
	end    
})

